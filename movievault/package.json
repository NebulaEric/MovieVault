{
  "name": "movievault",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k -n \"VITE,ELECTRON,TSC\" -c \"cyan,green,magenta\" \"vite\" \"npm:electron-dev\" \"npm:tsc-watch\"",
    "tsc-watch": "tsc --project electron/tsconfig.json --watch",
    "electron-dev": "wait-on http://localhost:5173 && electron .",
    "build": "vite build && tsc -p electron/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "electronmon": "^2.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "concurrently": "^9.1.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  }
}
